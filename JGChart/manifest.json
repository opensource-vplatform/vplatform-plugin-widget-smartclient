                    "type": "char"
                },
                {
                    "code": "Code",
                    "name": "控件名称",
                    "type": "char",
                    "default": "JGChart1"
                },
                {
                    "code": "StaticLayoutSize",
                    "name": "宽度固定",
                    "type": "boolean",
                    "editor": {
                        "type": "boolean"
                    },
                    "default": true
