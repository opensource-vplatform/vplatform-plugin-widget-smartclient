{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./src"],
        "includePattern": ".js$"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "docDest": "./doc/index.js",
        "exampleDest": "./example/index.js",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/v3-widget-smartclient-jsdoc-template"
    }
}