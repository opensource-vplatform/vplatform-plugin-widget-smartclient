{
    "groupId": "com.toone.v3.platform",
    "code": "widget-smartclient-JGImage",
    "version":"3.11.0",
    "plugins": [{
        "type": "widget",
        "code": "JGImage",
        "name": "V平台图片控件",
        "icon": "./JGImage.png",
        "desc": "",
        "defineUrl": "./dist/index.js",
        "docUrl":"./doc/index.js",
        "exampleUrl": "./example/index.js",
        "properties":[{
            "code":"Height",
            "name":"高度",
            "catalog":"GroupLayout",
            "type":"number",
            "desc":"设置控件高度",
            "default":100,
		    "editor":{
			    "type":"height"
		    }
        },{
            "code":"Width",
            "name":"宽度",
            "catalog":"GroupLayout",
            "type":"number",
            "desc":"设置控件宽度",
            "default":100,
		    "editor":{
			    "type":"width"
		    }
        },{
            "code":"Top",
            "name":"上边距",
            "catalog":"GroupLayout",
            "type":"number",
            "desc":"设置控件上边距",
            "default":100,
		    "editor":{
			    "type":"top"
		    }
        },{
            "code":"Left",
            "name":"左边距",
            "catalog":"GroupLayout",
            "type":"number",
            "desc":"设置控件左边距",
            "default":100,
		    "editor":{
			    "type":"left"
		    }
        },{
            "code":"TabIndex",
            "name":"界面顺序号",
            "catalog":"GroupLayout",
            "type":"number",
            "desc":"设置控件界面顺序号",
            "default":3,
		    "editor":{
			    "type":"integer"
		    }
        },{
            "code":"Visible",
            "name":"显示",
            "catalog":"Layout",
            "type":"boolean",
            "desc":"确定该控件是否显示",
            "default":true,
		    "editor":{
			    "type":"boolean"
		    }
        },{
            "code":"ImageValue",
            "name":"选择图片",
            "catalog":"Layout",
            "type":"char",
            "desc":"选择资源管理器中已有的图片文件",
		    "editor":{
			    "type":"resource"
		    }
        },{
            "code":"Dock",
            "name":"泊靠",
            "catalog":"Layout",
            "type":"char",
            "desc":"定义要绑定到容器的控件边框",
		    "editor":{
			    "type":"text"
		    }
        },{
            "code":"ImageMouse",
            "name":"鼠标状态",
            "catalog":"Layout",
            "type":"char",
            "desc":"鼠标状态",
		    "editor":{
			    "type":"select",
                "options":[{
                    "value":"Default",
                    "label":"Default"
                },{
                    "value":"Hand",
                    "label":"Hand"
                },{
                    "value":"IBeam",
                    "label":"IBeam"
                }]
		    }
        },{
            "code":"ImagePosition",
            "name":"图片位置",
            "catalog":"Layout",
            "type":"char",
            "desc":"图片位置",
		    "editor":{
			    "type":"select",
                "options":[{
                    "value":"fill",
                    "label":"拉伸"
                },{
                    "value":"contain",
                    "label":"适应"
                }]
		    }
        },{
            "code":"Code",
            "name":"控件编码",
            "catalog":"Other",
            "type":"char",
            "desc":"在设计器中用来表示该控件唯一编码。",
		    "editor":{
			    "type":"code",
                "prefix":"JGImage"
		    }
        },{
            "code":"Alias",
            "name":"控件名称",
            "catalog":"Other",
            "type":"char",
            "desc":"与控件关联的名称",
		    "editor":{
			    "type":"text"
		    }
        },{
            "code":"OnClick",
            "name":"单击事件",
            "catalog":"Event",
            "type":"char",
            "desc":"单击控件时触发",
		    "editor":{
			    "type":"ruleset"
		    }
        },{
            "code":"TableName",
            "name":"实体",
            "catalog":"Data",
            "type":"char",
            "desc":"控件绑定的实体",
		    "editor":{
			    "type":"entity"
		    }
        },{
            "code":"ColumnName",
            "name":"字段名称",
            "catalog":"Data",
            "type":"char",
            "desc":"控件绑定的字段",
		    "editor":{
                "entityProp":"TableName",
			    "type":"field"
		    }
        },{
            "code":"ToolTip",
            "name":"浮动提示",
            "catalog":"Data",
            "type":"char",
            "desc":"鼠标悬停在控件上时显示的提示信息",
		    "editor":{
			    "type":"expression"
		    }
        }],
        "dependencies":[{
            "groupId":"com.toone.v3.platform",
            "code":"vjs.framework.extension.platform.plugin.ui.smartclient.JGBaseWidget",
            "pluginSymbolicName":"vjs.framework.extension.platform.plugin.ui.smartclient.JGBaseWidget",
            "pluginCode":"JGBaseWidget",
            "pluginType":"widget"
        }]
    }]
}